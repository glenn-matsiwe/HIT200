<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".clubs.club_details">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".clubs.clubsNotification"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="16dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:context=".clubs.clubs_home"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="29dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                tools:context=".blog.blog_home">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/background"
                    tools:context=".home_page"
                    tools:layout_editor_absoluteX="0dp"
                    tools:layout_editor_absoluteY="0dp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="20dp"
                        tools:context=".log_in">

                        <ImageView
                            android:id="@+id/imageView"
                            android:layout_width="82dp"
                            android:layout_height="58dp"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentRight="true"
                            android:layout_marginTop="33dp"
                            android:layout_marginEnd="198dp"
                            android:layout_marginRight="198dp"
                            app:srcCompat="@drawable/logo" />

                        <TextView
                            android:id="@+id/textView5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_marginStart="124dp"
                            android:layout_marginLeft="124dp"
                            android:layout_marginTop="50dp"
                            android:text="Club Details"
                            android:textAllCaps="true"
                            android:textSize="18sp" />

                        <Button
                            android:id="@+id/deptMenu_back"
                            android:layout_width="235dp"
                            android:layout_height="65dp"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_marginStart="18dp"
                            android:layout_marginLeft="18dp"
                            android:layout_marginTop="410dp"
                            android:text="Back" />

                        <View
                            android:id="@+id/view7"
                            android:layout_width="280dp"
                            android:layout_height="305dp"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_marginStart="-1dp"
                            android:layout_marginLeft="-1dp"
                            android:layout_marginTop="90dp"
                            android:background="@drawable/logbg2" />

                        <TextView
                            android:id="@+id/description"
                            android:layout_width="132dp"
                            android:layout_height="101dp"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_marginStart="119dp"
                            android:layout_marginLeft="119dp"
                            android:layout_marginTop="240dp"
                            android:hint="@android:string/fingerprint_icon_content_description"
                            tools:targetApi="m" />

                        <TextView
                            android:id="@+id/advisors"
                            android:layout_width="132dp"
                            android:layout_height="52dp"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_marginStart="119dp"
                            android:layout_marginLeft="119dp"
                            android:layout_marginTop="176dp"
                            android:hint="advisors" />

                        <TextView
                            android:id="@+id/department_name"
                            android:layout_width="130dp"
                            android:layout_height="wrap_content"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_marginStart="121dp"
                            android:layout_marginLeft="121dp"
                            android:layout_marginTop="125dp"
                            android:hint="@string/app_name" />

                        <TextView
                            android:id="@+id/textView10"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_marginStart="31dp"
                            android:layout_marginLeft="31dp"
                            android:layout_marginTop="125dp"
                            android:text="Name :" />

                        <TextView
                            android:id="@+id/textView11"
                            android:layout_width="92dp"
                            android:layout_height="50dp"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_marginStart="28dp"
                            android:layout_marginLeft="28dp"
                            android:layout_marginTop="176dp"
                            android:text="Faculty Adivisors : " />

                        <TextView
                            android:id="@+id/description"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_marginStart="28dp"
                            android:layout_marginLeft="28dp"
                            android:layout_marginTop="240dp"
                            android:text="Description :" />

                    </RelativeLayout>
                </RelativeLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>